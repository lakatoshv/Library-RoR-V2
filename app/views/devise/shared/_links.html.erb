<%
	log_in = t ".log_in"
	sign_up = t ".sign_up"
	forgot =  t ".forgot"
	didnt_receive_confirmation = t ".didnt_receive_confirmation"
	didnt_receive_unlock = t ".didnt_receive_unlock"
%>
<%- if controller_name != 'sessions' %>
  <%= link_to log_in, new_session_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to sign_up, new_registration_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to forgot, new_password_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to didnt_receive, new_confirmation_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to didnt_receive_unlock, new_unlock_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%
        with = OmniAuth::Utils.camelize(provider) 
        href = omniauth_authorize_path(resource_name, provider)
    %>
      <div class="col-xs-4 col-sm-4">
        <a href="<%= href %>" class="btn btn-lg btn-default btn-block>">
          <i class="fa fa-<%= provider %> visible-xs"></i>
          <span class="hidden-xs" style="color: black;"><%= with %></span>
        </a>
      </div>
  <% end -%>
<% end -%>

